<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>형식 불일치</title>
</head>
<body>
<script type="text/javascript">
var a = Number("a");
document.write("a = " + a + "<br>");	//NaN (Not a Number)

var sum = a + 1;	//NaN + 1
document.write("sum = " + sum + "<br>");	//NaN

var str = a + "str";
document.write("str = " + str + "<br>");	//NaNstr

document.write("<br>");

var v1 = "가나다라";
var v2 = "123456";

var is_num1 = ! isNaN(v1);
var is_num2 = ! isNaN(v2);

document.write("is_num1 = " + is_num1 + "<br>");	//false
document.write("is_num2 = " + is_num2 + "<br>");	//true

if(is_num1) {
	document.write(v1 + "은(는) 숫자 입니다.<br>");
} else {
	document.write(v1 + "은(는) 숫자가 아닙니다.<br>");
}
document.write("<br>");
if(is_num2) {
	document.write(v2 + "은(는) 숫자 입니다.<br>");
} else {
	document.write(v2 + "은(는) 숫자가 아닙니다.<br>");
}
document.write("<br>");
</script>
</body>
</html>