<script type="text/javascript">
let today = new Date();
let time = today.getTime();

document.write(`time = ${time}<br>`); //1970년 1월 1일 자정이후 ms로 지난시간을 가져온다
document.write(`일 = ${Math.round(time / 1000 / 60 / 60 / 24)}`);
</script>
