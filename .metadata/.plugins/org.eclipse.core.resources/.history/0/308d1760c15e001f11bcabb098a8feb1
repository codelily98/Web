//빈 객체로 생성
var calc = {};

//맴버(필드) 변수 추가
calc.x = 0;
calc.y = 0;

//맴버 함수 (=메서드)
calc.setValue = function(x, y) {
	this.x = x;
	this.y = y;
}
calc.plus = function(x, y) {
	return x + y;
}
calc.minus = function(x, y) {
	return x - y;
}
calc.result() = function() {
	var sum = plus();
	var sub = minus();
	
	document.innerText("덧셈 = " + sum);
	document.innerText("뻴셈 = " + minus);
}