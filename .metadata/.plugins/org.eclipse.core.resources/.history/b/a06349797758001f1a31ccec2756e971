package baekjoon;

import java.util.Scanner;

public class b_30802 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int n = 0;
		int m = 0;
		int t = 0;
		int p = 0;
		int count = 0;
		int a[] = {};
		int result_t1 = 0;
		int result_t2 = 0;
		int result_p1 = 0;
		int result_p2 = 0;
		
		n = scanner.nextInt();
		for(int i=0; i <= 6; i++) {
			m = scanner.nextInt();
			i += m;
			a[count] = i;
			count++;			
		}
		
		t = scanner.nextInt();
		p = scanner.nextInt();
		
		for(int i=0; i < count; i++) {
			if(t / a[i] > 1) {
				result_t1++;
			} 
			else {
				result_t2 = a[i] / t + 1;
			}
		}
		
		result_p1 = 23 / p;
		result_p2 = 23 % p;
		
		System.out.println(result_t1 + result_t2);
		System.out.println(result_p1 + " " + result_p2);
	}
}